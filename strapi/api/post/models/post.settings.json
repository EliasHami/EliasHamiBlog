{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "post"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "content": {
      "type": "richtext"
    },
    "blog": {
      "model": "blog",
      "via": "posts"
    },
    "published": {
      "type": "boolean",
      "default": false
    }
  }
}
